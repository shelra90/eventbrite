  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.0.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "mogoose": "^0.0.1-security",
    "mongoose": "^7.0.4",
    "nodemon": "^2.0.22"
  }
